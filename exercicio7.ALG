algoritmo "dobroetercaparte"

var

 num: real
 dobro: real
 tercaparte: real

inicio

      escreva("Digite um número real: ")
      leia (num)
      dobro <- num*2
      tercaparte <- num/3
      escreval("O dobro de ", num, " é ", dobro)
      escreva("A terça parte de ", num, " é ", tercaparte)

fimalgoritmo