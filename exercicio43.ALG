algoritmo "contagemRegressiva"

var

 contador: inteiro

inicio

 contador <- 30
 Enquanto (contador>=1) faca
          Se (contador%4=0) entao
             escreval("[", contador, "]")
             senao
                  escreval(contador)
          FimSe
 contador <- contador - 1
 FimEnquanto

fimalgoritmo