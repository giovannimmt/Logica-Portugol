algoritmo "somaInversaEnquanto"

var

 contador, soma: inteiro

inicio

 contador <- 500
 soma <- 0
 Enquanto (contador>=0) faca
          escreval(contador)
          soma <- soma + contador
          contador <- contador - 50
 FimEnquanto
 
 escreval("A soma de todos os números é: ", soma)

fimalgoritmo